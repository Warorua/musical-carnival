Options -Indexes -MultiViews
RewriteEngine On

# your app lives in /kever_b, so set the base
RewriteBase /kever_b/

# pretty agent URLs: /kever_b/a/agent-one-9f3c
RewriteRule ^a/([A-Za-z0-9_-]+)/?$ agent/login.php?slug=$1 [L,QSA]

# (optional) also support a trailing slash-less folder style
# RewriteRule ^a/?$ agent/login.php [L,QSA]
