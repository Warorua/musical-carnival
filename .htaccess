Options -Indexes -MultiViews

<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  # /a/{slug}  ->  agent/login.php?slug={slug}
  RewriteRule ^a/([A-Za-z0-9_-]+)/?$ agent/login.php?slug=$1 [L,QSA]

  # pass through real files/dirs
  RewriteCond %{REQUEST_FILENAME} -f [OR]
  RewriteCond %{REQUEST_FILENAME} -d
  RewriteRule . - [L]
</IfModule>
